        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SlowTraffic</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#SlowVehicle -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#SlowVehicle">

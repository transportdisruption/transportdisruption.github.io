        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Roadworks</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureWorksClearance -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureWorksClearance">

        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#HighSpeedEmergencyVehicle</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#HockeyGame -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#HockeyGame">

        <rdfs:label rdf:datatype="&xsd;string">Variable Message Sign Failure</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFailure"/>
        <rdfs:comment rdf:datatype="&xsd;string">Failure of a variable message sign. This concept and description are based on the DATEX II concept variableMessageSigns.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VariableMessageSigns</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VariableMessageSignFault -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VariableMessageSignFault">
        <rdfs:label rdf:datatype="&xsd;string">Variable Message Sign Fault</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFault"/>
        <rdfs:comment rdf:datatype="&xsd;string">Faulty or malfunctioning variable message sign. This concept and description are based on the DATEX II concept variableMessageSigns.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VariableMessageSigns</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleCarryingHazardousMaterials -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleCarryingHazardousMaterials">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Carrying Hazardous Materials</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">Vehicles carrying materials of a hazardous nature are present and these could expose others to additional hazards. This concept and description are a generalised version of the DATEX II concept vehicleCarryingHazardousMaterials.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleCarryingHazardousMaterials</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleInDifficulty -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleInDifficulty">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle In Difficulty</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">A vehicle is experiencing difficulties (e.g. manoeuvring or propulsion difficulties) which may cause traffic disruption. This concept and description are from the DATEX II concept vehicleInDifficulty.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleInDifficulty</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleInspectionCheckPoint -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleInspectionCheckPoint">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Inspection Check Point</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#AuthorityOperation"/>
        <rdfs:comment rdf:datatype="&xsd;string">A permanent or temporary operation established on or adjacent to the transport infrastructure for inspection of vehicles by authorities (e.g. vehicle safety checks and tachograph checks). This concept and description are a generalised version of the DATEX II concept vehicleInspectionCheckPoint.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleInspectionCheckPoint</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleObstruction -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleObstruction">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Obstruction</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#Obstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">An obstruction on some infrastructure caused by one or more vehicles. This concept and description are a generalised version of the DATEX II concept VehicleObstruction.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleObstruction</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleOffInfrastructure -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleOffInfrastructure">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Off Infrastructure</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#Accident"/>
        <rdfs:comment rdf:datatype="&xsd;string">Includes all accidents where one or more vehicles have left the transport infrastructure.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleOffRoad -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleOffRoad">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Off Road</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleOffInfrastructure"/>
        <rdfs:comment rdf:datatype="&xsd;string">Includes all accidents where one or more vehicles have left the roadway. This concept and description are from the DATEX II concept vehicleOffRoad.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleOffRoad</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleOnFire -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleOnFire">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle On Fire</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">A vehicle is or has been on fire and may cause traffic disruption. This concept and description are from the DATEX II concept vehicleOnFire.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleOnFire</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleOnWrongCarriageway -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleOnWrongCarriageway">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle On Wrong Carriageway</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">A vehicle is travelling the wrong way along a divided highway (i.e. on the wrong side). This concept and description are from the DATEX II concept vehicleOnWrongCarriageway.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleOnWrongCarriageway</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleRecovery -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleRecovery">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Recovery</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureSideAssistance"/>
        <rdfs:comment rdf:datatype="&xsd;string">Vehicle recovery. This concept and description are from the DATEX II concept vehicleRecovery.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleRecovery</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleRepair -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleRepair">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Repair</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureSideAssistance"/>
        <rdfs:comment rdf:datatype="&xsd;string">Vehicle repair assistance. This concept and description are from the DATEX II concept vehicleRepair.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleRepair</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleSpunAround -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleSpunAround">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Spun Around</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#Accident"/>
        <rdfs:comment rdf:datatype="&xsd;string">Includes all accidents where a vehicle has skidded and has come to rest not facing its intended line of travel. This concept and description are from the DATEX II concept vehicleSpunAround.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleSpunAround</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleStorageInOperation -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleStorageInOperation">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Storage In Operation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#RoadOrCarriagewayOrLaneManagement"/>
        <rdfs:comment rdf:datatype="&xsd;string">Vehicles are being stored on the roadway and/or at a rest area or service area at the specified location. This concept and description are from the DATEX II concept vehicleStorageInOperation.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleStorageInOperation</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleStuck -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleStuck">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Stuck</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">One or more vehicles are stuck (i.e. unable to move). This concept and description are adaptations of the DATEX II concept vehicleStuck.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleStuck</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleStuckUnderBridge -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleStuckUnderBridge">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Stuck Under Bridge</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleStuck"/>
        <rdfs:comment rdf:datatype="&xsd;string">A vehicle is stuck under a bridge. This concept and description are from the DATEX II concept vehicleStuckUnderBridge.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleStuckUnderBridge</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleWeighing -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleWeighing">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle Weighing</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#AuthorityOperation"/>
        <rdfs:comment rdf:datatype="&xsd;string">A permanent or temporary operation established on or adjacent to the transport infrastructure for weighing of vehicles by authorities. This concept and description are a generalised version of the DATEX II concept vehicleWeighing.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleWeighing</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleWithOverheightLoad -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleWithOverheightLoad">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle With Overheight Load</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">An over-height vehicle which may present a hazard to road users. This concept and description are from the DATEX II concept vehicleWithOverheightLoad.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleWithOverheightLoad</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VehicleWithOverwideLoad -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VehicleWithOverwideLoad">
        <rdfs:label rdf:datatype="&xsd;string">Vehicle With Overwide Load</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#VehicleObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">A vehicle of width greater than that normally allowed which may cause traffic disruption. This concept and description are from the DATEX II concept vehicleWithOverwideLoad.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VehicleWithOverwideLoad</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VeryHazardous -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VeryHazardous">
        <rdfs:label rdf:datatype="&xsd;string">Very Hazardous</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#DrivingCondition"/>
        <rdfs:comment rdf:datatype="&xsd;string">Driving conditions are very hazardous due to environmental conditions. This concept and description are from the DATEX II concept veryHazardous.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VeryHazardous</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VeryStrongGustsOfWind -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VeryStrongGustsOfWind">
        <rdfs:label rdf:datatype="&xsd;string">Very Strong Gusts Of Wind</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#EnvironmentalConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Constantly varying winds, very strong at times. This concept and description are from the DATEX II concept veryStrongGustsOfWind.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VeryStrongGustsOfWind</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#VisibilityReduced -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#VisibilityReduced">
        <rdfs:label rdf:datatype="&xsd;string">Visibility Reduced</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#EnvironmentalConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Environmental conditions causing reduced visibility. This concept and description are from the DATEX II concept visibilityReduced.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#VisibilityReduced</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WaterSportsMeeting -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WaterSportsMeeting">
        <rdfs:label rdf:datatype="&xsd;string">Water Sports Meeting</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#PublicEvent"/>
        <rdfs:comment rdf:datatype="&xsd;string">Water sports meeting that could disrupt traffic. This concept and description are from the DATEX II concept waterSportsMeeting.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WaterSportsMeeting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WeakBridge -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WeakBridge">
        <rdfs:label rdf:datatype="&xsd;string">Weak Bridge</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureDamageObstruction"/>
        <rdfs:comment rdf:datatype="&xsd;string">Weak bridge capable of carrying a reduced load, typically with a reduced weight limit restriction imposed. This concept and description are from the DATEX II concept weakBridge.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WeakBridge</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WeatherRelatedConditions -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WeatherRelatedConditions">
        <rdfs:label rdf:datatype="&xsd;string">Weather Related Road Conditions</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#Conditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Conditions of infrastructure, for example road surfaces, that are related to the weather which may affect the transport conditions, such as ice, snow or water. This concept and description are a generalised version of the DATEX II concept WeatherRelatedRoadConditions.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WeatherRelatedRoadConditions</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WeighInMotion -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WeighInMotion">
        <rdfs:label rdf:datatype="&xsd;string">Weigh In Motion</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#AuthorityOperation"/>
        <rdfs:comment rdf:datatype="&xsd;string">A permanent or temporary facility established by authorities on the carriageway for weighing vehicles while in motion. This concept and description are from the DATEX II concept weighInMotion.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WeighInMotion</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WeightRestrictionInOperation -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WeightRestrictionInOperation">
        <rdfs:label rdf:datatype="&xsd;string">Weight Restriction In Operation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#RoadOrCarriagewayOrLaneManagement"/>
        <rdfs:comment rdf:datatype="&xsd;string">A weight restriction is in operation. This concept and description are from the DATEX II concept weightRestrictionInOperation.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WeightRestrictionInOperation</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#Wet -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#Wet">
        <rdfs:label rdf:datatype="&xsd;string">Wet</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Road surface is wet. This concept and description are from the DATEX II concept wet.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Wet</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WetAndIcyRoad -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WetAndIcyRoad">
        <rdfs:label rdf:datatype="&xsd;string">Wet And Icy Road</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Increased skid risk due to partly thawed, wet road with packed snow and ice, or rain falling on packed snow and ice. This concept and description are from the DATEX II concept wetAndIcyRoad.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WetAndIcyRoad</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WetIcyPavement -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WetIcyPavement">
        <rdfs:label rdf:datatype="&xsd;string">Wet Icy Pavement</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Partly thawed, wet pedestrian pavement with packed snow and ice, or rain falling on packed snow and ice. This concept and description are from the DATEX II concept wetIcyPavement.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WetIcyPavement</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WhiteOut -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WhiteOut">
        <rdfs:label rdf:datatype="&xsd;string">White Out</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#EnvironmentalConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Falling snow in blizzard conditions resulting in very reduced visibility. This concept and description are from the DATEX II concept whiteOut.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WhiteOut</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WinterConditions -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WinterConditions">
        <rdfs:label rdf:datatype="&xsd;string">Winter Conditions</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#DrivingCondition"/>
        <rdfs:comment rdf:datatype="&xsd;string">Driving conditions are consistent with those expected in winter. This concept and description are from the DATEX II concept winterConditions.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WinterConditions</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WinterSportsMeeting -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WinterSportsMeeting">
        <rdfs:label rdf:datatype="&xsd;string">Winter Sports Meeting</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#PublicEvent"/>
        <rdfs:comment rdf:datatype="&xsd;string">Winter sports meeting or event (e.g. skiing, ski jumping, skating) that could disrupt traffic. This concept and description are from the DATEX II concept winterSportsMeeting.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WinterSportsMeeting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#WinterStorm -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#WinterStorm">
        <rdfs:label rdf:datatype="&xsd;string">Winter Storm</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#EnvironmentalConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Heavy rain, sleet, hail and/or snow in combination with strong winds, limiting visibility to 50m or less. This concept and description are from the DATEX II concept winterStorm.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#WinterStorm</rdfs:seeAlso>
    </owl:Class>
    



        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SprayHazard</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#StateOccasion -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#StateOccasion">

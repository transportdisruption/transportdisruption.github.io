        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#RoadsideWork</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureSurfaceInPoorCondition -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSurfaceInPoorCondition">

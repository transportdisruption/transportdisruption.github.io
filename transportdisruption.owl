        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#RoadsideCommunicationsSystem</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureSidePowerSystemFailure -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSidePowerSystemFailure">
        <rdfs:label rdf:datatype="&xsd;string">Side of Infrastructure Power System Failure</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFailure"/>
        <rdfs:comment rdf:datatype="&xsd;string">Failure of power system at the side of infrastructure which is used by one or more equipments or system. This concept and description are based on the DATEX II concept roadsidePowerSystem.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#RoadsidePowerSystem</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureSidePowerSystemFault -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSidePowerSystemFault">
        <rdfs:label rdf:datatype="&xsd;string">Side of Infrastructure Power System Fault</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFault"/>
        <rdfs:comment rdf:datatype="&xsd;string">Faulty or malfunctioning power system at side of infrastructure which is used by one or more roadside equipments or systems. This concept and description are based on the DATEX II concept roadsidePowerSystem.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#RoadsidePowerSystem</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureSideWork -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSideWork">

        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#IntermittentShortTermClosures</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InternationalSportsMeeting -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InternationalSportsMeeting">

        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Ice on the road frozen in the form of wheel tracks. This concept and description are from the DATEX II concept iceWithWheelBarTracks.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#IceWithWheelBarTracks</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#IcyPatches -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#IcyPatches">

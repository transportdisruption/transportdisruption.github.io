        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#StreetLighting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#Strike -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#Strike">

        <rdfs:comment rdf:datatype="&xsd;string">Presence of ice (of any kind) on transport infrastructure. This concept and description are a generalised version of the DATEX II concept ice.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Ice</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#IceBuildUp -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#IceBuildUp">
        <rdfs:label rdf:datatype="&xsd;string">Ice Build Up</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Ice is building up on the infrastructure surface. This concept and description are a generalised version of the DATEX II concept iceBuildUp.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#IceBuildUp</rdfs:seeAlso>
    </owl:Class>
    



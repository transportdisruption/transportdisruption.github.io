        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#WeatherRelatedConditions"/>
        <rdfs:comment rdf:datatype="&xsd;string">Melting snow (slush) on the roadway is formed into wheel tracks. This concept and description are from the DATEX II concept slushStrings.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SlushStrings</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#SmogAlert -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#SmogAlert">

        <rdfs:comment rdf:datatype="&xsd;string">Constantly varying winds, strong at times. This concept and description are from the DATEX II concept strongGustsOfWind.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#StrongGustsOfWind</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#StrongWinds -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#StrongWinds">

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSideCommunicationsSystemFault">
        <rdfs:label rdf:datatype="&xsd;string">Side of Infrastructure Communications System Fault</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFault"/>

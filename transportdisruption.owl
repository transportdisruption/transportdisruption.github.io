        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Snowfall</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#Snowplough -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#Snowplough">

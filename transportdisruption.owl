    <!-- http://purl.org/td/transportdisruption#causeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/td/transportdisruption#causeOf">
        <rdfs:label rdf:datatype="&xsd;string">cause of</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Indicates that a Disruptive Event is a cause of a Disruptive Impact.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:range rdf:resource="http://purl.org/td/transportdisruption#DisruptiveImpact"/>
        <owl:inverseOf rdf:resource="http://purl.org/td/transportdisruption#hasCause"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/td/transportdisruption#hasCause -->

    <owl:ObjectProperty rdf:about="http://purl.org/td/transportdisruption#hasCause">
        <rdfs:label rdf:datatype="&xsd;string">has cause</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Indicates that a Disruptive Impact is caused by an Event.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:domain rdf:resource="http://purl.org/td/transportdisruption#DisruptiveImpact"/>
    </owl:ObjectProperty>
    



        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureMaintenance"/>
        <rdfs:comment rdf:datatype="&xsd;string">Infrastructure works are completed and are being cleared. This concept and description are a generalised version the DATEX II concept roadworksClearance.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#RoadworksClearance</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InstallationWork -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InstallationWork">

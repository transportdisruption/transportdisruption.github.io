        <rdfs:comment rdf:datatype="&xsd;string">Hockey game event that could disrupt traffic. This concept and description are from the DATEX II concept hockeyGame.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#HockeyGame</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#HorseRaceMeeting -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#HorseRaceMeeting">
        <rdfs:label rdf:datatype="&xsd;string">Horse Race Meeting</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#PublicEvent"/>
        <rdfs:comment rdf:datatype="&xsd;string">Horse race meeting that could disrupt traffic. This concept and description are from the DATEX II concept horseRaceMeeting.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#HorseRaceMeeting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#HurricaneForceWinds -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#HurricaneForceWinds">
        <rdfs:label rdf:datatype="&xsd;string">Hurricane Force Winds</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#EnvironmentalConditions"/>

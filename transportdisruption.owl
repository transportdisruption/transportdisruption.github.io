        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#InstallationWork</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#IntermittentShortTermClosures -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#IntermittentShortTermClosures">

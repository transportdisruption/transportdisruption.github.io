        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#HurricaneForceWinds</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#Ice -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#Ice">

        <rdfs:comment rdf:datatype="&xsd;string">Failure of street or road lighting. This concept and description are based on the DATEX II concept streetLighting.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#StreetLighting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#StreetLightingFault -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#StreetLightingFault">
        <rdfs:label rdf:datatype="&xsd;string">Street Lighting Fault</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFault"/>

        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#MaintenanceWorks</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#InfrastructureSideAssistance -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#InfrastructureSideAssistance">

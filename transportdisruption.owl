        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SnowOnTheRoad</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#Snowfall -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#Snowfall">

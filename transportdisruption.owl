        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#DrivingCondition"/>
        <rdfs:comment rdf:datatype="&xsd;string">Current conditions are making driving impossible. This concept and description are from the DATEX II concept impossible.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Impossible</rdfs:seeAlso>
    </owl:Class>
    



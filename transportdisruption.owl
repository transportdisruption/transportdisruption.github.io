        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureWorks"/>
        <rdfs:comment rdf:datatype="&xsd;string">Work involving the construction of new infrastructure. This concept and description are from the DATEX II concept ConstructionWorks.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#ConstructionWorks</rdfs:seeAlso>
    </owl:Class>
    



        <rdfs:subClassOf rdf:resource="http://purl.org/td/transportdisruption#InfrastructureFault"/>
        <rdfs:comment rdf:datatype="&xsd;string">Faulty or malfunctioning signs used to control traffic speed. This concept and description are based on the DATEX II concept speedControlSigns.</rdfs:comment>
        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SpeedControlSigns</rdfs:seeAlso>
    </owl:Class>
    



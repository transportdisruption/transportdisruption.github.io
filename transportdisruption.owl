        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#SportsMeeting</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#SprayHazard -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#SprayHazard">

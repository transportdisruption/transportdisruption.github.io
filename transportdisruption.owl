        <rdfs:seeAlso>http://datex2.eu/schema/2/2_0#Strike</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.org/td/transportdisruption#StrongGustsOfWind -->

    <owl:Class rdf:about="http://purl.org/td/transportdisruption#StrongGustsOfWind">
